{
  "name": "obaranda-push",
  "version": "1.0.0",
  "description": "A web api to poll RSS at www obaranda.com and disemminate push messages to connected client Android apps. Resides at http://obaranda-push.herokuapp.com",
  "dependencies": {
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "firebase-admin": "^5.10.0",
    "moment": "^2.21.0",
    "node-vibrant": "^3.0.0",
    "oboe": "^2.1.4",
    "probe-image-size": "^4.0.0",
    "request": "^2.83.0",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "@google-cloud/firestore": "^0.12.0",
    "@types/compression": "^0.0.35",
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/oboe": "^2.0.28",
    "@types/request": "^2.47.0",
    "typescript": "^2.7.2"
  },
  "engines": {
    "node": "9.6.1"
  },
